{
  "name": "call-for-blood-foundation",
  "version": "1.0.0",
  "description": "Blood Donation Management System - A revolutionary digital platform for connecting blood donors and recipients",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm start",
    "client": "cd client && npm start",
    "server": "cd server && npm run dev",
    "build": "cd client && npm run build",
    "build:client": "cd client && npm run build",
    "start:prod": "cd server && npm start",
    "test": "npm run test:server && npm run test:client",
    "test:server": "cd server && npm test",
    "test:server:watch": "cd server && npm run test:watch",
    "test:server:coverage": "cd server && npm run test:coverage",
    "test:client": "cd client && npm test",
    "lint": "npm run lint:server",
    "lint:server": "cd server && npm run lint",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "install-all": "npm run install-server && npm run install-client",
    "start": "cd server && npm start",
    "seed:education": "cd server && node scripts/seedEducation.js",
    "seed:admin": "cd server && node scripts/seedAdmin.js",
    "db:seed": "npm run seed:education"
  },
  "keywords": [
    "blood-donation",
    "healthcare",
    "emergency-response",
    "whatsapp-integration",
    "donor-matching"
  ],
  "author": "CallforBlood Foundation",
  "license": "PROPRIETARY",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}